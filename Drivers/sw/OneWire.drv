%-*****************************************************************************************************
%-BW_METHOD_BEGIN add_CRC
%ifdef add_CRC
%define! Parbit_value
%include Common\OneWireadd_CRC.inc
void %'ModuleName'%.%add_CRC(uint8_t bit_value)
{
%CODE_BEGIN
	bool fb;
	fb = (Data.CRC & 0x01) ^ bit_value;
	Data.CRC >>= 1;
	if(fb) Data.CRC ^= 0x8C;
%CODE_END
}
