%-*****************************************************************************************************
%-BW_METHOD_BEGIN GetBytes
%ifdef GetBytes
%define! Par_byte
%define! RetVal
%include Common\OneWireGetBytes.inc
bool %'ModuleName'%.%GetBytes(uint8_t *bytes_array, uint8_t count)
{
%CODE_BEGIN
	if(count > Input_Count()) return FALSE;
	for(;count>0;count--) {
		*bytes_array = Input_Get();
		bytes_array++;
	}
	return TRUE;
