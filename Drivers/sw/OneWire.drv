%define! RetVal
%include Common\OneWireWaitms.inc
bool %'ModuleName'%.%Waitms(uint8_t key, uint16_t time_ms)
{
%CODE_BEGIN
	PROG pr;
	if(Program_isFull() || Time_isFull()) return FALSE;
	%@CS@'ModuleName'%.CriticalVariable();
	%@CS@'ModuleName'%.EnterCritical();
