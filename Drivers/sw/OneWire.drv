%-*****************************************************************************************************
%-BW_METHOD_BEGIN SendBytes
%ifdef SendBytes
%define! Par_bytes
%define! Parcount
%define! RetVal
%include Common\OneWireSendBytes.inc
bool %'ModuleName'%.%SendBytes(uint8_t *_bytes, uint8_t count)
{
%CODE_BEGIN
	PROG pr;
	if((Program_Last->Instr != I_SEND && Program_isFull()) || Output_CountFree() < count) return FALSE;
	pr.Instr = I_SEND;
	pr.Count = count;
	for(;count>0;count--) {
		Output_Put(*_bytes);
		_bytes++;
	}
	if(Program_Last->Instr == I_SEND) Program_Last->Count += pr.Count;
	else Program_Put(pr);
	if(!Data.Busy) {
		%'ModuleName'%.i_action();
		%'ModuleName'%.i_run();
		%'ModuleName'%.i_send_low();
	}
	return TRUE;
%CODE_END
}
%endif %-SendBytes
%-BW_METHOD_END SendBytes

