{
  byte res = ERR_OK;

  while(*Chr != '\0') {
    if (%'ModuleName'%.%SendChar(*Chr)!=ERR_OK) {
      res = ERR_TXFULL;
    }
    Chr++;
  }
%if defined(OnError)
  if (res!=ERR_OK) {
    %OnError(%'ModuleName'%.USB_ERR_TX_STRING);
  }
%endif
  return res;
}

