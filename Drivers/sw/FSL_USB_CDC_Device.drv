%-BW_METHOD_BEGIN SendBlock
%ifdef SendBlock
%define! Pardata
%define! PardataSize
%define! RetVal
%include Common\FSL_USB_CDC_DeviceSendBlock.Inc
byte %'ModuleName'%.%SendBlock(byte *data, word dataSize)
{
  byte res = ERR_OK;

  while(dataSize > 0) {
    if (%'ModuleName'%.%SendChar(*data)!=ERR_OK) {
      res = ERR_TXFULL;
    }
    dataSize--; data++;
  }
%if defined(OnError)
  if (res!=ERR_OK) {
    %OnError(%'ModuleName'%.USB_ERR_TX_BLOCK);
  }
%endif
  return res;
}

%endif %- SendBlock
