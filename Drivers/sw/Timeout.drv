%-BW_METHOD_BEGIN AddTick
%ifdef AddTick
%include Common\TimeoutAddTick.Inc
void %'ModuleName'%.%AddTick(void)
{
  byte i;

  EnterCritical();
  for(i=0;i<%'ModuleName'%.NOF_COUNTERS;i++) {
    if (%'ModuleName'%.Counters[i]>0) {
      %'ModuleName'%.Counters[i]--;
    }
  }
  ExitCritical();
}

%endif %- AddTick
