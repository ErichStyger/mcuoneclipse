void %'ModuleName'%.%LeaveCounter(%'ModuleName'_CounterHandle handle)
{
  if (handle==%'ModuleName'%.OUT_OF_HANDLE) {
    return;
  }
  EnterCritical();
  %'ModuleName'%.Counters[handle] = 0;
  %'ModuleName'%.FreeCounters[handle]=TRUE;
  ExitCritical();
}

%endif %- LeaveCounter
%-BW_METHOD_END LeaveCounter
