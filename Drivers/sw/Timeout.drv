bool %'ModuleName'%.%CounterExpired(%'ModuleName'_CounterHandle handle)
{
  bool res;

  if (handle==%'ModuleName'%.OUT_OF_HANDLE) {
    return TRUE;
  }
  EnterCritical();
  res = (bool)(%'ModuleName'%.Counters[handle]==0);
  ExitCritical();
  return res;
}

%endif %- CounterExpired
