%-BW_METHOD_BEGIN SetContrast
%ifdef SetContrast
%define! Parcontrast
%include Common\PDC8544SetContrast.Inc
void %'ModuleName'%.%SetContrast(byte contrast)
{
  if (contrast < 128) { /* check for valid contrast values */
    WriteCmd(0x21);
    WriteCmd((byte)(0x80|contrast));
    WriteCmd(0x20);
  }
}

%endif %- SetContrast
%-BW_METHOD_END SetContrast
