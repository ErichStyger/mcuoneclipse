%-BW_METHOD_BEGIN SetMode
%ifdef SetMode
%define! Parinverted
%include Common\PDC8544SetMode.Inc
void %'ModuleName'%.%SetMode(bool inverted)
{
  if (inverted) {
    WriteCmd(0x0C|1);                                            %>40 /* Inverted display mode */
  } else {
    WriteCmd(0x0C);                                              %>40 /* Normal display mode */
  }
}

%endif %- SetMode
