%-BW_METHOD_BEGIN SetPos
%ifdef SetPos
%define! Parx
%define! Pary
%include Common\PDC8544SetPos.Inc
void %'ModuleName'%.%SetPos(%'ModuleName'_PixelDim x, %'ModuleName'_PixelDim y)
{
  WriteCmd((byte)(0x40|(y&0x07)));                               %>40 /* Y axis */
  WriteCmd((byte)(0x80|(x&(%'ModuleName'%.DISPLAY_HW_NOF_COLUMNS-1))));%>40 /* X axis */
}

%endif %- SetPos
%-BW_METHOD_END SetPos
